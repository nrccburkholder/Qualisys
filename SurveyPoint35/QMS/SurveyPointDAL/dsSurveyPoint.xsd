						<xs:sequence>
							<xs:element name="CriteriaTypeID" type="xs:int" />
							<xs:element name="Name" type="xs:string" />
							<xs:element name="Description" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
