							<xs:element name="Description" type="xs:string" minOccurs="0" />
							<xs:element name="DevDescription" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Triggers">
